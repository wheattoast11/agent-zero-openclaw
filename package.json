    },
    "./interop": {
      "import": "./dist/interop/index.js",
      "types": "./dist/interop/index.d.ts"
    }
  },
  "bin": {
    "agent-zero": "./bin/agent-zero.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "THEORY.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
